{
  "manifest_version": 2,
  "name": "ChatGPT Analyzer",
  "version": "1.0",
  "description": "Analyze user interactions with ChatGPT",
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' https://d3js.org https://cdnjs.cloudflare.com; object-src 'self';",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      
      "js": ["content.js","add.js", "wordcloud.js"]
    }
  ]
}

